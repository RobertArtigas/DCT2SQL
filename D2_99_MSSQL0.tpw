      #SET(%NFieldLine, %NFieldLine & ' ' & %NFieldNN)          #! NOT NULL
    #ENDIF
    #IF(%NFieldDef)
      #SET(%NFieldLine, %NFieldLine & ' ' & %NFieldDef)         #! DEFAULT
    #ENDIF
    #IF(%NFieldOpt4)                                            #! SJ.2003.03.08 - ANY OTHER OPTIONS
      #SET(%NFieldLine, %NFieldLine & ' ' & %NFieldOpt4)        #! SJ.2003.03.08
    #ENDIF                                                      #! SJ.2003.03.08
    #!
    #IF(%FieldComma > 0)
      #SET(%FieldComma, %FieldComma - 1)
      #IF(SUB(%NFieldOpt1,1,1)='E')                             #! RASQL(END)
  %NFieldLine
      #ELSE
  %NFieldLine ,
      #ENDIF
    #ELSE
  %NFieldLine
    #ENDIF
  #ENDIF
#!
  #!----------------------------------------------------------------------------!
  #!----------------------------------------------------------------------------!
#!
#ENDFOR
#!---------------------------------------------------------------------
#!---------------------------------------------------------------------
#GROUP(%MsSqlField0End)
#!---------------------------------------------------------------------
#!---------------------------------------------------------------------
#!---------------------------------------------------------------------
#!---------------------------------------------------------------------
#GROUP(%MsSqlField0Constraints)
#!--------------------------------------------------------------------!
#!--------------------------------------------------------------------!
#! Dump the field constraints here for a MSSQL database back end.     !
#!--------------------------------------------------------------------!
#!--------------------------------------------------------------------!
#SET(%InnerGroup, 0)
#SET(%OverGroup, 0)
#SET(%FieldComma, ITEMS(%Field) - 1)
#!
#FOR(%Field)
  #SET(%NFieldOpt1,EXTRACT(%FieldUserOptions,'RASQL',1))
  #!--------------------------------------------------------------!
  #! The first option RASQL(NO) controls if a field is processed. !
  #!--------------------------------------------------------------!
  #! WARNING: The last field can NOT be an ommited field.         !
  #!--------------------------------------------------------------!
  #! And if the last field happens to be and 'END', you will need !
  #! to add a RASQL(END) to the last field before the 'END'. This !
  #! will suppress the last comma. Reality sometimes happens.     !
  #!--------------------------------------------------------------!
  #IF(SUB(%NFieldOpt1,1,1)='N')
    #SET(%FieldComma, %FieldComma - 1)
    #CYCLE
  #ENDIF
  #IF(NOT(%FieldInitial) AND NOT(%FieldValidation))            #! SJ.2003.03.03  SKIP if NO Default Value OR Validation
    #SET(%FieldComma, %FieldComma - 1)
    #CYCLE
  #ENDIF
  #IF(%FieldValidation='NONZERO')                              #! SJ.2003.03.03  SKIP NONZERO Valiation
    #SET(%FieldComma, %FieldComma - 1)
    #CYCLE
  #ENDIF
  #IF(%OverGroup=1)
     #IF(%FieldType='END')
        #SET(%OverGroup, 0)
        #SET(%InnerGroup, %InnerGroup - 1)
     #ENDIF
     #SET(%FieldComma, %FieldComma - 1)
     #CYCLE
  #ENDIF
#!
  #SET(%NFieldName, '')                                        #! Field name
  #!------------------------------------------------!
  #! Begin removal of types that are not processed. !
  #!------------------------------------------------!
  #IF(%FieldType='GROUP')
    #SET(%InnerGroup, %InnerGroup + 1)
    #IF(INSTRING('OVER(',%FieldStatement,1,1))
      #SET(%OverGroup, 1)
    #ENDIF
    #SET(%FieldComma, %FieldComma - 1)
    #CYCLE
  #ENDIF
  #!
  #IF(%FieldType='END')
    #SET(%InnerGroup, %InnerGroup - 1)
    #SET(%FieldComma, %FieldComma - 1)
    #CYCLE
  #ENDIF
  #SET(%FD1L,%FieldDimension1)                                  #! Name 1 made shorter
  #SET(%FD2L,%FieldDimension2)                                  #! Name 2 made shorter
  #SET(%FD3L,%FieldDimension3)                                  #! Name 3 made shorter
  #SET(%FD4L,%FieldDimension4)                                  #! Name 4 made shorter
  #IF(%FD1L>0)                                                  #! IF we got array field,
    #SET(%FieldComma, %FieldComma - 1)                          #! THEN ignore the array
    #CYCLE
  #ENDIF
  #!------------------------------------------------!
  #! Ended removal of types that are not processed. !
  #!------------------------------------------------!
#!
  #IF(%FieldName)
     #SET(%nLoc1,INSTRING('<39>',%FieldName,1,1))
     #SET(%nLoc2,INSTRING('<39>',%FieldName,1,%nLoc1+1))
     #SET(%NFieldName,SUB(%FieldName,%nLoc1+1,%nLoc2-%nLoc1-1))
  #ELSE
    #SET(%NFieldName, %FieldID)
    #IF(%UsePrefix)
      #SET(%NFieldName, %FilePrefix & '_' & %FieldID)
    #ENDIF
  #ENDIF
  #SET(%SqlWordItem, UPPER(%NFieldName))                        #! Set up search of
  #FIND(%SqlWordList, %SqlWordItem)                             #! SQL reserved words
  #IF(%SqlWordList)                                             #! Found? Yes! We add
    #ADD(%ReservedFieldList, %FoundFileField & %FilePrefix & ':' & %NFieldName)
    #SET(%NFieldName, %NFieldName & '_')                        #! trailing underscore
  #ENDIF                                                        #! to get it through
#!
  #SET(%FieldCount, %FieldCount + 1)                            #! Add to field count
#!
  #SET(%NFieldOpt2,'')
  #SET(%NFieldOpt3,'')
  #SET(%NFieldOpt4,'')
#!
  #IF(%FieldInitial)
     #SET(%NFieldOpt2,%FieldInitial)
  #ENDIF
  #IF(%FieldValidation)
    #CASE(%FieldValidation)
    #OF('BOOLEAN')
      #SET(%NFieldOpt3,'<39>' & %FieldTrueValue & '<39>,<39>' & %FieldFalseValue & '<39>')
    #OF('INLIST')
      #IF(ITEMS(%FieldValues))
        #FOR(%FieldValues)
          #IF(%NFieldOpt3)
            #SET(%NFieldOpt3,CLIP(%NFieldOpt3) & ',<39>' & %FieldValues & '<39>')
          #ELSE
            #SET(%NFieldOpt3,'<39>' & %FieldValues & '<39>')
          #END
        #ENDFOR
#!        #SET(%NFieldOpt3,%FieldDisplayChoices)
#!        #LOOP
#!          #SET(%nLoc1,LEN(%NFieldOpt3))
#!          #SET(%nLoc2,INSTRING('|',%NFieldOpt3,1,1))            #! Replace |(Pipe) with ','
#!          #IF(%nLoc2)
#!            #SET(%NFieldOpt3,SUB(%NFieldOpt3,1,%nLoc2-1) & '<39>,<39>' & SUB(%NFieldOpt3,1,%nLoc1-%nLoc2))
#!          #ELSE
#!            #BREAK
#!          #ENDIF
#!        #ENDLOOP
      #ENDIF
    #OF('INRANGE')
      #IF(%FieldRangeLow)
        #SET(%NFieldOpt4,%NFieldName & ' >= ' & %FieldRangeLow)
      #ENDIF
      #IF(%FieldRangeHigh)
        #IF(%NFieldOpt4)
          #SET(%NFieldOpt4,CLIP(%NFieldOpt4) & ' AND ' & %NFieldName & ' <<= ' & %FieldRangeHigh)
        #ELSE
          #SET(%NFieldOpt4,%NFieldName & ' <<= ' & %FieldRangeHigh)
        #ENDIF
      #ENDIF
    #ELSE
      #SET(%FieldCount, %FieldCount - 1)                        #! Subtract from field count
      #SET(%FieldComma, %FieldComma - 1)                        #! SKIP
      #CYCLE
    #ENDCASE
  #ENDIF
#!
  #SET(%NFileName,%File)
  #IF(%UsePrefix)
    #SET(%NFileName,%FilePrefix & '_' & %File)
  #ENDIF
  #SET(%SqlWordItem, UPPER(%NFileName))
  #FIND(%SqlWordList, %SqlWordItem)
  #IF(%SqlWordList)
    #ADD(%ReservedFieldList, %FoundFileName & %NFileName)
    #SET(%NFileName, %NFileName & '_')
  #ENDIF
#!
  #!----------------------------------------------------------------------------!
  #!----------------------------------------------------------------------------!
  #! This is the actual SQL field data line creation.                           !
  #!----------------------------------------------------------------------------!
  #!----------------------------------------------------------------------------!
#!
  #IF(%NFieldOpt2)
      #IF(%DelConst)
IF EXISTS (SELECT * FROM DBO.SYSOBJECTS
   WHERE ID = OBJECT_ID(N'%SQLPrefix.DF_%NFileName_%NFieldName')
         AND OBJECTPROPERTY(ID, N'IsDefaultCnst') = 1)
   ALTER TABLE %SQLPrefix.%NFileName
         DROP CONSTRAINT DF_%NFileName_%NFieldName
GO
      #ENDIF
ALTER TABLE %SQLPrefix.%NFileName
      ADD CONSTRAINT DF_%NFileName_%NFieldName
      DEFAULT %NFieldOpt2 FOR %NFieldName
GO

  #ENDIF
#!
  #IF(%NFieldOpt3)
      #IF(%DelConst)
IF EXISTS (SELECT * FROM DBO.SYSOBJECTS
   WHERE ID = OBJECT_ID(N'%SQLPrefix.CH_%NFileName_%NFieldName')
         AND OBJECTPROPERTY(ID, N'IsCheckCnst') = 1)
   ALTER TABLE %SQLPrefix.%NFileName
         DROP CONSTRAINT CH_%NFileName_%NFieldName
GO
      #ENDIF
ALTER TABLE %SQLPrefix.%NFileName
      ADD CONSTRAINT CH_%NFileName_%NFieldName
      CHECK (%NFieldName IN (%NFieldOpt3))
GO

  #ENDIF
#!
  #IF(%NFieldOpt4)
      #IF(%DelConst)
IF EXISTS (SELECT * FROM DBO.SYSOBJECTS
   WHERE ID = OBJECT_ID(N'%SQLPrefix.CH_%NFileName_%NFieldName')
         AND OBJECTPROPERTY(ID, N'IsCheckCnst') = 1)
   ALTER TABLE %SQLPrefix.%NFileName
         DROP CONSTRAINT CH_%NFileName_%NFieldName
GO
      #ENDIF
ALTER TABLE %SQLPrefix.%NFileName
      ADD CONSTRAINT CH_%NFileName_%NFieldName
      CHECK (%NFieldOpt4)
GO

  #ENDIF
#!
  #IF(%FieldComma > 0)
    #SET(%FieldComma, %FieldComma - 1)
  #ENDIF
#!
  #!----------------------------------------------------------------------------!
  #!----------------------------------------------------------------------------!
#!
#ENDFOR
